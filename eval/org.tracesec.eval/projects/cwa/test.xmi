<?xml version="1.0" encoding="ASCII"?>
<graphConfiguration:Configuration
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:graphConfiguration="http://www.tracesec.org/graph/dsl/GraphConfiguration">
  <namespaces
      package="http://www.eclipse.org/uml2/5.0.0/UML"
      default="2"/>
  <namespaces
      package="http://www.gravity-tool.org/typegraph/antipatterngraph/sonarlint"/>
  <namespaces
      package="http://www.gravity-tool.org/typegraph/basic"
      default="1">
    <include>
      <type
          href="http://www.gravity-tool.org/typegraph/basic#//TMember"/>
      <inlcude>
        <reference
            href="http://www.gravity-tool.org/typegraph/basic#//TMember/accessing"/>
        <type
            href="http://www.gravity-tool.org/typegraph/basic#//TAccess"/>
        <target
            href="http://www.gravity-tool.org/typegraph/basic#//TAccess/target"/>
        <weight
            xsi:type="graphConfiguration:NumberWeight"
            value="1"/>
      </inlcude>
    </include>
    <include>
      <type
          href="http://www.gravity-tool.org/typegraph/basic#//annotations/TAnnotatable"/>
      <inlcude>
        <reference
            href="http://www.gravity-tool.org/typegraph/basic#//annotations/TAnnotatable/tAnnotation"/>
        <type
            href="http://www.gravity-tool.org/typegraph/antipatterngraph/sonarlint#//SonarlintFinding"/>
        <weight
            xsi:type="graphConfiguration:NumberWeight"
            value="99999"/>
      </inlcude>
    </include>
  </namespaces>
  <namespaces
      package="http://www.tracesec.org/qualitymodel"
      consider="NONE">
    <include>
      <type
          href="http://www.tracesec.org/qualitymodel#//Quality"/>
      <inlcude>
        <reference
            href="http://www.tracesec.org/qualitymodel#//Quality/aspects"/>
        <type
            href="http://www.tracesec.org/qualitymodel#//Aspect"/>
        <target
            href="http://www.tracesec.org/qualitymodel#//Aspect/quality"/>
        <weight
            xsi:type="graphConfiguration:AttributeWeight">
          <value
              href="http://www.tracesec.org/qualitymodel#//Aspect/priority"/>
        </weight>
      </inlcude>
      <inlcude>
        <reference
            href="http://www.tracesec.org/qualitymodel#//Quality/relevantElements"/>
        <type
            href="http://www.gravity-tool.org/typegraph/antipatterngraph/sonarlint#//SonarlintFinding"/>
        <weight
            xsi:type="graphConfiguration:NumberWeight"
            value="10"/>
      </inlcude>
      <inlcude>
        <reference
            href="http://www.tracesec.org/qualitymodel#//Quality/relevantElements"/>
        <weight
            xsi:type="graphConfiguration:NumberWeight"
            value="4"/>
      </inlcude>
    </include>
  </namespaces>
</graphConfiguration:Configuration>
