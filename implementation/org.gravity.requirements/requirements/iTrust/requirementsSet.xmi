    <requirements xsi:type="requirements:Requirement" wording="The user inputs invalid information and is prompted to try again." id="E1" incomingReferences="//@requirements.15/@requirements.2/@outgoingReferences.1 //@requirements.15/@requirements.0/@outgoingReferences.0"/>
    <requirements xsi:type="requirements:Requirement" wording="The comment is empty and the text &#x201c;No Comment&#x201d; (without link) is displayed instead of the &#x201c;Read Comment&#x201d; link." id="E2" incomingReferences="//@requirements.15/@requirements.4/@outgoingReferences.0"/>
